CurrencyAmount(coinSelected, currencyType)\r\n\t});\r\n\t<\/script>\t\t<div class=\"elementor-shortcode\">\r\n\t\t<div class=\"convertor_box_wrap\">\r\n\t\t  <div class=\"row\">\r\n\t\t\t<div class=\"col-sm-6\">\r\n\t\t\t  <div class=\"main_inner_wrap\">\r\n\t\t\t\t<div class=\"inner_wrap\">\r\n\t\t\t\t  <select class=\"selectpicker\" id=\"coin_selected\">\r\n\t\t\t\t\t<option data-content=\"<i class='icon icon-bitcoin'><\/i><span>Bitcoin BTC<\/span>\" value=\"BTC\">Bitcoin<\/option>\r\n\t\t\t\t\t<option data-content=\"<i class='icon icon-ethereum'><\/i><span>Ethereum ETH<\/span>\" value=\"ETH\">Ethereum<\/option>\r\n\t\t\t\t\t<option data-content=\"<i class='icon icon-litecoin'><\/i><span>Litecoin LTC<\/span>\" value=\"LTC\">Litecoin<\/option>\r\n\t\t\t\t\t<option data-content=\"<i class='icon icon-ripple'><\/i><span>Ripple XRP<\/span>\" value=\"XRP\">Ripple<\/option>\r\n\t\t\t\t\t<option data-content=\"<i class='icon icon-bitcoin-cash-abc'><\/i><span>BitcoinCash BCH<\/span>\" value=\"BCH\">Bitcoin Cash<\/option>\r\n\t\t\t\t\t<option data-content=\"<i class='icon icon-xlm'><\/i><span>Stellar XLM<\/span>\" value=\"XLM\">XLM<\/option>\r\n\t\t\t\t\t<option data-content=\"<i class='icon icon-eos'><\/i><span>EOS<\/span>\" value=\"EOS\">EOS<\/option>\r\n\t\t\t\t\t<option data-content=\"<i class='icon icon-usdt'><\/i><span>Tether USDT<\/span>\" value=\"USDT\">USDT<\/option>\r\n\t\t\t\t\t<option data-content=\"<i class='icon icon-ada'><\/i><span>Cardano ADA<\/span>\" value=\"ADA\">Cardano<\/option>\r\n\t\t\t\t\t<!-- <option data-content=\"<span>Dash<\/span>\" value=\"DASH\">Dash<\/option> -->\r\n\t\t\t\t\t<!-- <option data-content=\"<span>Monero<\/span>\" value=\"XMR\">Monero<\/option> -->\r\n\t\t\t\t\t<!-- <option data-content=\"<span>NEO<\/span>\" value=\"NEO\">NEO<\/option> -->\r\n\t\t\t\t  <\/select>\r\n\t\t\t\t  <article>\r\n\t\t\t\t\t<input type=\"number\" value=\"1\" id=\"coinamount\" min=\"1\">\r\n\t\t\t\t  <\/article>\r\n\t\t\t\t<\/div>\r\n\t\t\t\t<span class=\"in